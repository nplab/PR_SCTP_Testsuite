+0.0 sendmsg(3, {msg_name(...)=...,
                 msg_iov(1)=[{iov_base=..., iov_len=3000}],
                 msg_control(48)=[{cmsg_len=28,
                   cmsg_level=IPPROTO_SCTP,
                   cmsg_type=SCTP_SNDINFO,
                   cmsg_data={snd_sid=1,
                     snd_flags=0,
                     snd_ppid=htonl(1234),
                     snd_context=2,
                     snd_assoc_id=0}},
                   { cmsg_len=20,
                     cmsg_level=IPPROTO_SCTP,
                     cmsg_type=SCTP_PRINFO,
                     cmsg_data={pr_policy=SCTP_PR_SCTP_RTX, pr_value=2}}],
